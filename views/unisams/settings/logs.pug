extends ../layout

block page_includes
    link(rel='stylesheet', href='/static/unisams/stylesheets/settings/logs.css')
    link(rel='stylesheet', href='/static/unisams/stylesheets/settings/settings.css')
    script(src="/dist/settings.js")



    script.
        //used to tell the bundled js which modules to load.
        window.jsmodule = {
            module: "settings",
            loaders: ["logs"],
        };
        window.dockerArgs.activeContainer = "settingsContainer";
        window.dockerArgs.activeElementId = "settingsLogs";


block content
    #wrapper
        #page-container.page-container
            #page-container-inner
                section#sec1
                    #sec-content.subpage-container
                        #content1-heading.sec-heading.subpage-heading
                            h3 activity logs
                        #content1-text.sec-content
                            .subpage-options
                            .subpage-content
                                #loglist-container.loglist-container




